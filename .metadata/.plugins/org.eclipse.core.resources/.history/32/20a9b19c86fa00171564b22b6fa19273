package exam;

public class Bubble {

	public static void main(String[] args) {
		int[] number = { 45, 81, 9, 72, 4, 6 };
		for (int x = 0; x < number.length; x++) {
			for (int y = x + 1; y < number.length; y++) {
				if (number[x] <= number[y]) {
					int tmp = number[x];
					number[x] = number[y];
					number[y] = tmp;
				}
			}
			System.out.print(number[x] + " ");
		}
		int[] n = { 6, 5, 2, 8, 1, 9, 13, 4, 11, 7 };
		for (int i = 0; i < n.length; i++) {
			for (int o = i + 1; o < n.length; o++) {
				if (n[i] <= n[o]) {
					int tmp = n[i];
					n[i] = n[o];
					n[o] = tmp;

				}
			}
			System.out.print(n[i] + " ");
		}
	}

}
